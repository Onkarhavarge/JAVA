
public class triangle {
	
	int side1 ;
	int side2 ;
	int side3 ;
	
	triangle(int side1,int side2,int side3)
	{
		this.side1=side1;
		this.side2=side2;
		this.side3=side3;
	}
	
	void display_area()
	{
		float area=this.side1 +this.side2+this.side3;
		System.out.println("Area:"+area);
	}
	public static void main(String[] args) {
		
		triangle tri = new triangle(3,4,5);

		tri.display_area();
	}

}
