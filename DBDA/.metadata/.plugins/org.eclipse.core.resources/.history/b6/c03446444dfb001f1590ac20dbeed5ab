
public class Calci
{
	public int add(int num1,int num2)
	{
		return num1 + num2;
	}
	public float add(float num1,float num2)
	{
		return num1 + num2;
	}
	public int Substract(int num1,int num2)
	{
		return num1 - num2;
	}
	public float Substract(float num1,float num2)
	{
		return num1 - num2;
	}
}
