
public class Ass6 {

	public static void main(String[] args) {
		int count=0;
		int sum=0;
		for(int i=2;i<100;i++)
		{
			int divisor=0;
			for (int j=1;j<=i;j++)
			{
				if( i % j==0 )
				{
					divisor++;
				}
				if(divisor == 2)
				{
					sum += i;
					count++;
				}
					
			}
		}
		System.out.println("count is :"+count);
		System.out.println("sum is :"+sum);
	}

}
